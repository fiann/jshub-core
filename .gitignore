# Ignore standard Rails directories
# ref: http://toolmantim.com/articles/setting_up_a_new_rails_app_with_git
log/*.log
tmp/
doc/api

# Ignore the generated app files
doc/app
public/website

# Ignore custom files, they're only there so we can apply the build process to them
app/javascripts/custom/*

# Ignore generated static documentation
website/_site

# Ignore the SQLite databases
db/*.sqlite3

# Ignore CI test reports
test/reports/*
test/reports/**/*

# Ignore generated zip archives
public/download/*.zip

# Ignore temp files generated by Passenger
.tmp_*

# Ignore generated Gem files
vendor/plugins/jshub_javascript_tester/pkg/*

# Ignore generated CHANGELOG files (simplifies dist scm merging)
public/CHANGELOG.txt
